<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:orientation="vertical" android:background="#b6b6b6"
	android:screenOrientation="portrait">

	<TextView
		android:text="HOW OFTEN do you observe squirrels obtaining food from the following..."
		android:id="@+id/textView1" android:layout_height="wrap_content"
		android:textColor="#FFFFFF" android:background="#000000"
		android:textStyle="bold" android:textSize="12px" android:layout_width="fill_parent"></TextView>

	<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:orientation="horizontal" android:background="#b6b6b6">

		<TextView android:text="Never, " android:id="@+id/textView2"
			android:layout_height="wrap_content" android:layout_width="wrap_content"
			android:background="#000000" android:textColor="#FFFFFF"
			android:textStyle="bold" android:textSize="14px">
		</TextView>
		<TextView android:text="Seldom, " android:id="@+id/textView2"
			android:layout_height="wrap_content" android:layout_width="wrap_content"
			android:background="#000000" android:textColor="#c9f5e5"
			android:textStyle="bold" android:textSize="14px">
		</TextView>
		<TextView android:text="Often, " android:id="@+id/textView2"
			android:layout_height="wrap_content" android:layout_width="wrap_content"
			android:background="#000000" android:textColor="#6de4b8"
			android:textStyle="bold" android:textSize="14px">
		</TextView>

		<TextView android:text="or Regularly?" android:id="@+id/textView2"
			android:layout_height="wrap_content" android:layout_width="fill_parent"
			android:background="#000000" android:textColor="#10d28b"
			android:textStyle="bold" android:textSize="14px">
		</TextView>
	</LinearLayout>

	<TableLayout android:id="@+id/tableLayout1"
		android:layout_width="fill_parent" android:layout_height="wrap_content">

		<TableRow android:layout_width="fill_parent"
			android:layout_marginBottom="10px">
			<TextView android:text="Bird\nFeeders" android:id="@+id/textView2"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:textSize="18px" android:textColor="#000000"></TextView>
			<SeekBar android:layout_height="wrap_content"
				android:layout_width="fill_parent" android:id="@+id/food_seekBar1"
				android:progress="0" android:max="3" android:progressDrawable="@drawable/gradient"
				android:layout_weight=".5">
			</SeekBar>
			<TextView android:text="   " android:id="@+id/textView7"
				android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
		</TableRow>

		<TableRow android:layout_width="fill_parent"
			android:layout_marginBottom="10px">
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:id="@+id/textView3"
				android:text="Human\nHandouts" android:textSize="18px"
				android:textColor="#000000"></TextView>
			<SeekBar android:layout_height="wrap_content"
				android:layout_width="fill_parent" android:id="@+id/food_seekBar2"
				android:progress="0" android:max="3" android:progressDrawable="@drawable/gradient"
				android:layout_weight=".5">
			</SeekBar>
			<TextView android:text="   " android:id="@+id/textView8"
				android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
		</TableRow>

		<TableRow android:layout_width="fill_parent"
			android:layout_marginBottom="10px">
			<TextView android:text="Trees and\nPlants" android:id="@+id/textView4"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:textSize="18px" android:textColor="#000000"></TextView>
			<SeekBar android:layout_height="wrap_content"
				android:layout_width="fill_parent" android:id="@+id/food_seekBar3"
				android:progress="0" android:max="3" android:progressDrawable="@drawable/gradient"
				android:layout_weight=".5">
			</SeekBar>
			<TextView android:text="   " android:id="@+id/textView9"
				android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
		</TableRow>


		<TableRow android:layout_width="fill_parent"
			android:layout_marginBottom="10px">
			<TextView android:text="Garbage\n" android:id="@+id/textView5"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:textSize="18px" android:textColor="#000000"></TextView>
			<SeekBar android:layout_height="wrap_content"
				android:layout_width="fill_parent" android:id="@+id/food_seekBar4"
				android:progress="0" android:max="3" android:progressDrawable="@drawable/gradient"
				android:layout_weight=".5">
			</SeekBar>
			<TextView android:text="   " android:id="@+id/textView10"
				android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
		</TableRow>

		<TableRow android:layout_width="fill_parent"
			android:layout_marginBottom="25px">
			<TextView android:text="Others" android:id="@+id/textView6"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:textSize="18px" android:textColor="#000000"></TextView>
			<SeekBar android:layout_height="wrap_content"
				android:layout_width="fill_parent" android:id="@+id/food_seekBar5"
				android:progress="0" android:max="3" android:progressDrawable="@drawable/gradient"
				android:layout_weight=".5">
			</SeekBar>
			<TextView android:text="   " android:id="@+id/textView11"
				android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
		</TableRow>

	</TableLayout>


	<EditText android:hint="Other (Please Specify)" android:id="@+id/food_editText1"
		android:layout_height="wrap_content" android:layout_width="fill_parent">
	</EditText>


	<RelativeLayout android:id="@+id/relativeLayout1"
		android:layout_height="wrap_content" android:layout_width="fill_parent">

		<Button android:text="Next" android:id="@+id/Next_Food"
			android:layout_height="wrap_content" android:layout_alignParentRight="true"
			android:layout_alignParentBottom="true" android:layout_width="100px">
		</Button>

	</RelativeLayout>

</LinearLayout>